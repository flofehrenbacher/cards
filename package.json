{
  "name": "cards",
  "version": "0.0.1",
  "license": "ISC",
  "scripts": {
    "clean": "rm -rf build",
    "tsc": "tsc",
    "test": "jest",
    "test:watch": "jest --watch --silent",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "build": "npm run clean && npm run tsc && npm test && webpack --mode=production",
    "start": "START_NODEMON=true webpack --mode=development --watch --progress",
    "analyze-client-bundle": "ANALYZE_CLIENT_BUNDLE=true webpack -p"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run tsc"
    }
  },
  "lint-staged": {
    "*.{js,json,md,mdx,ts,tsx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@hapi/joi": "^17.1.1",
    "@koa/router": "^8.0.8",
    "@types/koa-cache-control": "^2.0.1",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "emotion-reset": "^2.0.7",
    "koa": "^2.11.0",
    "koa-cache-control": "^2.0.0",
    "koa-joi-router": "^6.0.2",
    "koa-mount": "^4.0.0",
    "koa-static": "^5.0.0",
    "md5": "^2.2.1",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "shuffle-array": "^1.0.1",
    "socket.io": "^2.3.0",
    "uws": "10.148.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@emotion/styled": "^10.0.27",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.1",
    "@types/hapi__joi": "^16.0.12",
    "@types/html-webpack-plugin": "^3.2.2",
    "@types/jest": "^25.1.4",
    "@types/jest-axe": "^3.2.1",
    "@types/koa": "^2.11.3",
    "@types/koa-joi-router": "^5.2.3",
    "@types/koa-mount": "^4.0.0",
    "@types/koa-static": "^4.0.1",
    "@types/koa__router": "^8.0.2",
    "@types/node": "^13.9.5",
    "@types/ramda": "^0.27.0",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/shuffle-array": "0.0.28",
    "@types/socket.io": "^2.1.4",
    "@types/webpack": "^4.41.9",
    "@types/webpack-bundle-analyzer": "^2.13.3",
    "babel-jest": "^25.2.4",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "favicons-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^4.0.3",
    "husky": "^4.2.3",
    "jest": "^25.2.4",
    "jest-axe": "^3.4.0",
    "jest-junit": "^10.0.0",
    "jest-watch-typeahead": "",
    "lint-staged": "^10.0.10",
    "nodemon": "^2.0.2",
    "nodemon-webpack-plugin": "^4.3.1",
    "prettier": "^2.0.2",
    "ramda": "^0.27.0",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11"
  }
}
